<form method="post" action="{{ path('app_admin_enseignant_delete', {'id': enseignant.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ enseignant.id) }}">
    <button class="{{ classes|default('btn btn-danger-soft') }}">{{ icon|default('')|raw }} Delete</button>
</form>
