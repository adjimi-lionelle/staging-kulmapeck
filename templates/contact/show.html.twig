<!DOCTYPE html>

<title>Contact</title>

{% block body %}
    <h1>Contact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact.id }}</td>
            </tr>
            <tr>
                <th>Object</th>
                <td>{{ contact.object }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ contact.content }}</td>
            </tr>
            <tr>
                <th>CreateAt</th>
                <td>{{ contact.createAt ? contact.createAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contact_index') }}">back to list</a>

    <a href="{{ path('app_contact_edit', {'id': contact.id}) }}">edit</a>

    {{ include('contact/_delete_form.html.twig') }}
{% endblock %}
