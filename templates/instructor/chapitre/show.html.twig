    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ chapitre.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ chapitre.title }}</td>
            </tr>
            <tr>
